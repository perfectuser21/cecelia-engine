{
  "name": "zenithjoy-engine",
  "version": "12.6.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "build": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "qa": "npm run typecheck && npm run test && npm run build",
    "qa:local": "npm run typecheck",
    "qa:gate": "bash scripts/qa-with-gate.sh",
    "pr:check": "npm run qa",
    "release:check": "npm run qa && bash scripts/release-check.sh",
    "coverage:rci": "node scripts/devgate/scan-rci-coverage.cjs",
    "ci:preflight": "bash scripts/devgate/ci-preflight.sh"
  },
  "keywords": [
    "ai",
    "content",
    "pipeline",
    "workflow"
  ],
  "author": "",
  "license": "ISC",
  "description": "AI development workflow engine with content generation pipeline",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "winston": "^3.10.0",
    "pg": "^8.11.3",
    "openai": "^4.20.0",
    "p-limit": "^5.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.9",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/pg": "^8.10.2",
    "js-yaml": "^4.1.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
